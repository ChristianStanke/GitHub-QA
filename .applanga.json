{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./JSON/en/en.json",
          "file_format": "react_nested_json",
          "tag": "app:language.json",
          "language": "en"
        },
        {
          "path": "./Android-XML/en/en.xml",
          "file_format": "android_xml",
          "tag": "androidXML",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./JSON/<language>/<language>.json",
          "file_format": "react_nested_json",
          "tag": "app:language.json",
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./Android-XML/<language>/<language>.xml",
          "file_format": "android_xml",
          "tag": "androidXML",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}